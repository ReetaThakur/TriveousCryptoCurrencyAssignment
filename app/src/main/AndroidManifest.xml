<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reeta.triveouscryptocurrencyassignment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bitcoin"
        android:name=".di.CurrencyApp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bitcoin"
        android:supportsRtl="true"
        android:theme="@style/Theme.TriveousCryptoCurrencyAssignment">
        <activity
            android:name=".adapter.PracticeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>